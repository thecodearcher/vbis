    "express-rate-limit": "^5.3.0",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "joi-phone-number": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "3.7.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
