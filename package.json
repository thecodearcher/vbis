    "@types/express-rate-limit": "^5.1.3",
    "@types/faker": "^5.5.8",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.0.1",
    "@types/joi-phone-number": "^5.0.3",
    "@types/jsonwebtoken": "^8.5.5",
